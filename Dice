using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Dice
{
    class Program
    {

      static void Main(string [] args)
        {

            Random random = new Random();
            int name = random.Next(minValue:1, maxValue: 6);

            System.Console.WriteLine("YOU ROLLED: " + name);
            Console.ReadKey(); 
        }
    }
}
